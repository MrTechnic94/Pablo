{
  "name": "pablo",
  "version": "1.0.9",
  "description": "Just a useful bot.",
  "main": "index.js",
  "scripts": {
    "build": "pnpm install && pnpm update && pnpm lint:update && npx dotenvx encrypt -f ./config/.env",
    "pm2:docker": "npx pm2-runtime start . --name pablo -- --color",
    "env:encrypt": "npx dotenvx encrypt -f ./config/.env",
    "env:decrypt": "npx dotenvx decrypt -f ./config/.env",
    "pm2": "npx pm2 start . --name pablo -- --color",
    "lint:update": "pnpm exec biome migrate --write",
    "nodemon": "nodemon .",
    "lint": "biome lint .",
    "start": "node ."
  },
  "type": "commonjs",
  "author": {
    "name": "MrTechnic",
    "url": "https://github.com/MrTechnic94/"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@dotenvx/dotenvx": "^1.49.0",
    "discord.js": "^14.22.1",
    "node-cron": "^4.2.1",
    "pino": "^9.9.0",
    "undici": "^7.15.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9",
    "pino-pretty": "^13.1.1",
    "zlib-sync": "^0.1.10"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "nodemon": "^3.1.10",
    "pm2": "^6.0.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MrTechnic94/Pablo"
  },
  "bugs": {
    "url": "https://github.com/MrTechnic94/Pablo/issues"
  },
  "homepage": "https://github.com/MrTechnic94/Pablo#readme",
  "engines": {
    "node": ">=20.18.0",
    "pnpm": ">=9.12.0"
  }
}
