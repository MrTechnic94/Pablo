{
  "name": "pablo",
  "version": "2.0.0",
  "description": "Just a useful bot.",
  "main": "index.js",
  "scripts": {
    "build": "pnpm install && pnpm update && pnpm lint:update",
    "setup-service": "chmod +x setup-service.sh && ./setup-service.sh",
    "pm2:docker": "npx pm2-runtime start . --name pablo -- --color",
    "lint:update": "pnpm exec biome migrate --write",
    "watcher": "node --env-file=./config/.env --watch .",
    "pm2": "npx pm2 start . --name pablo -- --color",
    "start": "node --env-file=./config/.env .",
    "lint": "biome lint ."
  },
  "type": "commonjs",
  "author": {
    "name": "MrTechnic",
    "url": "https://github.com/MrTechnic94/"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@redis/client": "^5.10.0",
    "croner": "^10.0.1",
    "discord.js": "^14.25.1",
    "pino": "^10.3.0",
    "undici": "^7.20.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.1.0",
    "pino-pretty": "^13.1.3",
    "zlib-sync": "^0.1.10"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MrTechnic94/Pablo"
  },
  "bugs": {
    "url": "https://github.com/MrTechnic94/Pablo/issues"
  },
  "homepage": "https://github.com/MrTechnic94/Pablo#readme",
  "engines": {
    "node": ">=20.18.0",
    "pnpm": ">=9.12.0"
  }
}
